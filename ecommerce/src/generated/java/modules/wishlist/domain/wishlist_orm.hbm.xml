		<property name="type" length="20" />
		<filter name="wishlistShoppingListNoneFilter" condition="1=0"/>
		<filter name="wishlistShoppingListCustomerFilter" condition="bizCustomer=:customerParam"/>
		<filter name="wishlistShoppingListDataGroupIdFilter" condition="bizDataGroupId=:dataGroupIdParam"/>
		<filter name="wishlistShoppingListUserIdFilter" condition="bizUserId=:userIdParam"/>
	</class>

	<class name="modules.wishlist.domain.ShoppingListItem" table="ShoppingListItem" entity-name="wishlistShoppingListItem">
		<id name="bizId" length="36" />
		<version name="bizVersion" unsaved-value="null" />
		<property name="bizLock" type="OptimisticLock" length="271" not-null="true" />
		<property name="bizKey" length="1024" not-null="true" />
		<property name="bizCustomer" length="50" not-null="true" />
		<property name="bizFlagComment" length="1024" />
		<property name="bizDataGroupId" length="36" />
		<property name="bizUserId" length="36" not-null="true" />
		<many-to-one name="categoryid" entity-name="productCategory" column="categoryid_id" cascade="persist,save-update,refresh,merge" foreign-key="FK_ShoppingListItem_categoryid"/>
		<many-to-one name="productid" entity-name="productProduct" column="productid_id" cascade="persist,save-update,refresh,merge" foreign-key="FK_ShoppingListItem_productid"/>
		<property name="sequencenum" length="2" />
		<filter name="wishlistShoppingListItemNoneFilter" condition="1=0"/>
		<filter name="wishlistShoppingListItemCustomerFilter" condition="bizCustomer=:customerParam"/>
		<filter name="wishlistShoppingListItemDataGroupIdFilter" condition="bizDataGroupId=:dataGroupIdParam"/>
		<filter name="wishlistShoppingListItemUserIdFilter" condition="bizUserId=:userIdParam"/>
	</class>

	<filter-def name="wishlistShoppingListNoneFilter" />
	<filter-def name="wishlistShoppingListCustomerFilter">
		<filter-param name="customerParam" type="string"/>
	</filter-def>
	<filter-def name="wishlistShoppingListDataGroupIdFilter">
		<filter-param name="dataGroupIdParam" type="string"/>
	</filter-def>
	<filter-def name="wishlistShoppingListUserIdFilter">
		<filter-param name="userIdParam" type="string"/>
	</filter-def>
	<filter-def name="wishlistShoppingListItemNoneFilter" />
	<filter-def name="wishlistShoppingListItemCustomerFilter">
		<filter-param name="customerParam" type="string"/>
	</filter-def>
	<filter-def name="wishlistShoppingListItemDataGroupIdFilter">
