<?xml version="1.0" encoding="UTF-8"?>
<module xmlns="http://www.skyve.org/xml/module" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" title="wishlist" name="wishlist" xsi:schemaLocation="http://www.skyve.org/xml/module ../../schemas/module.xsd">
    <homeRef>edit</homeRef>
    <homeDocument>ShoppingList</homeDocument>
    <documents>
        <document ref="ShoppingList"/>
        <document ref="ShoppingListItem"/>
    </documents>
    <roles>
        <role name="Maintainer">
            <description><![CDATA[Create, edit and delete permission within the Products module.]]></description>
            <privileges>
                <document name="ShoppingList" permission="CRUDC"/>
                <document name="ShoppingListItem" permission="CRUDC"/>
            </privileges>
        </role>
    </roles>
    <menu>
        <list document="ShoppingList" name="All ShoppingList">
            <role name="Maintainer"/>
        </list>
        <list document="ShoppingListItem" name="ShoppingListItem Address">
            <role name="Maintainer"/>
        </list>

    </menu>
</module>