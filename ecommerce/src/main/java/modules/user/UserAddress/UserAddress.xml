<?xml version="1.0" encoding="UTF-8"?>
<document xmlns="http://www.skyve.org/xml/document"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	name="UserAddress"
	xsi:schemaLocation="http://www.skyve.org/xml/document ../../../schemas/document.xsd">
	<persistent name="UserAddress" />

	<singularAlias>UserAddress</singularAlias>
	<pluralAlias>useraddresses</pluralAlias>
	<bizKey expression="{addressid}" />
	<attributes>
		
		
		
		
		<text name="addressid" required="true">
			<displayName>Address Id</displayName>
			<length>10</length>
		</text>
	<!-- Foreign key -->
		<association type="aggregation" name="userloginid"
			required="true">
			<displayName>User Login Id</displayName>
			<documentName>UserLogin</documentName>
		</association>

		<text name="sequenceno" required="true">
			<displayName>Sequence number</displayName>
						<length>2</length>
			
		</text>

		<text name="addresstype" required="true">
			<displayName>Address Type</displayName>
			<length>20</length>
		</text>
		<text name="address1" required="true">
			<displayName>Address line 1</displayName>
			<length>50</length>
		</text>
		
		<text name="address2" required="false">
			<displayName>Address line 2</displayName>
			<length>50</length>
		</text>
		
		<text name="city" required="true">
			<displayName>City</displayName>
			<length>15</length>
		</text>
		
		<text name="state" required="true">
			<displayName>State</displayName>
			<length>15</length>
		</text>
		
		<text name="zipcode" required="true">
			<displayName>Zip code</displayName>
			<length>6</length>
		</text>
		
		<text name="country" required="true">
			<displayName>Country</displayName>
			<length>15</length>
		</text>

		<dateTime name="createdstamp">
			<displayName>Created timestamp </displayName>
		</dateTime>

		<dateTime name="updatedstamp">
			<displayName>Updated timestamp </displayName>
		</dateTime>

	</attributes>
</document>